[run]
source = app
omit = 
    */__pycache__/*
    */tests/*
    */migrations/*
    */venv/*
    */.venv/*
    */__pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @property
    @staticmethod
    @classmethod
    
show_missing = True
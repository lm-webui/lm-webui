{
  "name": "lm-webui",
  "private": true,
  "version": "0.3.0",
  "description": "All-in-one Local AI Interface and LLM Runtime, with Multimodal-RAG System, and Multi-Provider Support.",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && uvicorn app.main:app --host 0.0.0.0 --port 8000 --reload",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "echo 'Python backend does not require build'",
    "start": "npm run dev",
    "start:prod": "cd backend && uvicorn app.main:app --host 0.0.0.0 --port 8000",
    "lint": "cd frontend && npm run lint",
    "lint:fix": "cd frontend && npm run lint:fix",
    "format": "cd frontend && npm run format",
    "format:check": "cd frontend && npm run format:check",
    "typecheck": "cd frontend && npm run typecheck",
    "test": "cd frontend && npm run test",
    "clean": "cd frontend && npm run clean",
    "install:all": "npm install && cd backend && pip install -r requirements.txt",
    "docker:dev": "docker-compose up",
    "docker:build": "docker-compose build"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "keywords": [
    "llm",
    "ai",
    "webui",
    "gguf",
    "fastapi",
    "typescript"
  ],
  "author": "lm-webui",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3"
  }
}
